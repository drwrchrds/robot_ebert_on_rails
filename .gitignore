# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

# Ignore build stuff
bundle.js
bundle.js.map
/public/assets/*
/app/assets/stylesheets/build.css

.byebug_history
.DS_Store
.rubocop_todo.yml
